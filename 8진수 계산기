package goorm;

import java.util.Scanner;

public class Text {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int n = input.nextInt();
		int sum = 0;
		for(int i = 0; i< n; i++) {
			sum += input.nextInt();
		}
		
		System.out.println((int)(sum/8) * 10 + (sum%8));
		
		input.close();
	}

}
