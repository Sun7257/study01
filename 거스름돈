package goorm;

import java.util.Scanner;

class Text {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);

		int n = input.nextInt();

		int sum = 0;
		sum += n / 40;
		n = n % 40;

		sum += n / 20;
		n = n % 20;

		sum += n / 10;
		n = n % 10;

		sum += n / 5;
		n = n % 5;

		sum += n / 1;
		n = n % 1;
		
		System.out.println(sum);

		input.close();
	}
}
