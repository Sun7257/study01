import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		
		int n = input.nextInt();
		int m = input.nextInt();
		int[] A = new int[n];
		
		for (int i =0; i<n; i++)
			A[i] = input.nextInt();
		
		for (int i =0; i<n; i++) {
			if (A[i] % m == 0);
			else 
				A[i] *= m;
		}
		
		for (int i=0;i<n;i++) {
			System.out.print(A[i]+" ");
		}
		
		input.close();
	}

}
